function START()
logs() 
MN = gg.choice({
"â Perspective & Charms", 
"â Weapon Hacks [Safe]", 
"â Game Hacks [Brutal]", 
"â Antenna Menu [Safe]", 
"â One Click [Lobby/Safe]", 
"EXIT" } , nil, " â TBCâ„¢ OFFICIAL VIP SCRIPT v5.8  â˜ƒï¸\n â PUBGM VERSION : 0.15.5 â›„\n â SCRIPT BY @TEAMBATTLEAXECHEATS ğŸ\n")

if MN == nil then else 
if MN == 1 then pnc() end
if MN == 2 then lobby() end
if MN == 3 then game() end
if MN == 4 then antennaX() end
if MN == 5 then -- Select Functions 
mwhs4() 
lr() 
aiml() 
end
if MN == 6 then EXIT() end
end
XEROX = -1
end

