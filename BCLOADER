function net()
  gg.clearResults()
    gg.setRanges(8)
    SN("1.3132476e-38;1.956023e-35;1.1754945e-37::9", 16, false, 536870912, 0, -1)
  if gg.getResultCount() == 0 then
  else
    SN("1.1754945e-37", 16, false, 536870912, 0, -1)
    table.insert(ODC, gg.getResults(21))
    EA("0", 16)
    table.insert(EDC, gg.getResults(21))
    gg.clearResults()
    gg.sleep(500)
    for x = #ODC, 1, -1 do
      gg.setValues(ODC[x])
    end
    gg.toast("Success")
  end
  end

  function Off2()
    for x = #EDC, 1, -1 do
      gg.setValues(EDC[x])
    end
    gg.sleep(1000)
    for x = #ODC, 1, -1 do
      gg.setValues(ODC[x])
  end
end
  
  function Off3()
    for x = #ODC, 1, -1 do
      gg.setValues(ODC[x])
  end
end
  
  ODC = {}
  EDC = {}

