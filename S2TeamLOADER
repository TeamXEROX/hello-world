function CLR425(...)
  CLRMN425 = gg.choice({
    "‚ôí  ŸÑŸàŸÜ ÿ£ÿ≠ŸÖÿ±  425",
    "‚ò£Ô∏è  ŸÑŸàŸÜ ÿ£ÿµŸÅÿ±  425",
    "‚û°Ô∏è  ÿ±ÿ¨Ÿàÿπ"
  }, nil, "‚ïî‚îÄ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚ñë‚ñë üî´ ‚ñë‚ñë‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÄ‚ïó\n              ‚öîÔ∏è„Äò S2Team OFFICIAL‚Ñ¢ „Äô‚öîÔ∏è \n‚öîÔ∏è Telegram ‚üÆ @S2Team ‚üØ‚öîÔ∏è \n‚ïö‚îÄ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚ñë‚ñë  üå† ‚ñë‚ñë‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÄ‚ïù")
  if CLRMN425 == nil then
  else
    if CLRMN425 == 1 then
      RED425()
    end
    if CLRMN425 == 2 then
      YELL425()
    end
    if CLRMN425 == 3 then
      CLR()
    end
  end
  VolcanoS2Team = -1
end

function CLR4252(...)
