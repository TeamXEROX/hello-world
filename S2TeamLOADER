function CLOSE(...)
  print("‚ïî‚îÄ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚ñë‚ñë üëë ‚ñë‚ñë‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÄ‚ïó\n\n             ‚ôîüî∏„Äò SEASON2TEAM „Äôüî∏‚ôî\nüî∏ Telegram ‚üÆ @SEASON2TEAM ‚üØ  üî∏ \n\n‚ïö‚îÄ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚ñë‚ñë  ‚òÖ ‚ñë‚ñë‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÄ‚ïù")
  gg.skipRestoreState()
  gg.setVisible(true)
  os.exit()
end

while true do
  if gg.isVisible(true) then
    PUBGMH = 1
    gg.setVisible(false)
  end
  if PUBGMH == 1 and CMENU == 0 then
    HOME()
  end
  if PUBGMH == 1 and CMENU == 1 then
    HOME1()
  end
  if PUBGMH == 1 and CMENU == 2 then
    HOME2()
  end
end
