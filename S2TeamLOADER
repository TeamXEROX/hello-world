function CLOSE(...)
  print("‚ïî‚îÄ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚ñë‚ñë üî´ ‚ñë‚ñë‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÄ‚ïó\n\n              ‚öîÔ∏è„Äò S2Team OFFICIAL‚Ñ¢ „Äô‚öîÔ∏è \n‚öîÔ∏è Telegram ‚üÆ @S2Team ‚üØ‚öîÔ∏è \n\n‚ïö‚îÄ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚ñë‚ñë  üå† ‚ñë‚ñë‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÄ‚ïù")
  gg.skipRestoreState()
  gg.setVisible(true)
  os.exit()
end

while true do
  if gg.isVisible(true) then
    VolcanoS2Team = 1
    gg.setVisible(false)
  end
  if VolcanoS2Team == 1 and CMENU == 0 then
    HOME()
  end
  if VolcanoS2Team == 1 and CMENU == 1 then
    HOME1()
  end
  if VolcanoS2Team == 1 and CMENU == 2 then
    HOME2()
  end
end
